{
    "manifest_version": 2,
    "name": "__MSG_name__",
    "version": "0.0.1",
    "default_locale": "en",
    "description": "__MSG_description__",
    "developer": {
        "name": "TBK",
        "url": "https://github.com/TBK/qwant"
    },
    "chrome_settings_overrides": {
        "search_provider": {
            "name": "Qwant",
            "keyword": "q",
            "search_url": "https://www.qwant.com/?b=0&q={searchTerms}",
            "favicon_url": "https://www.qwant.com/favicon.ico",
            "suggest_url": "https://api.qwant.com/api/suggest/?q={searchTerms}&client=opensearch&lang=__MSG_locale__",
            "is_default": true
        }
    }
}